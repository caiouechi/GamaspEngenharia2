    <script type="text/javascript">

        //data: { destinatario: email }, //our data
        function enviaEmail() {
            if (validaCampos() == true) {
                $('#status').show();
                $('#preloader').show();
                var destinatario = document.getElementById("email").value;
                var mensagem = document.getElementById("message").value;
                var name = document.getElementById("name").value;
                var url = 'http://apis.tecnologia.ws/email.php?' + 'Email=' + destinatario + '&Message=' + mensagem + '&Name=' + name + '&SiteNumber=cicero';

                $.ajax({
                    url: url, //your server side script
                    data: {},
                    type: 'GET',
                    success: function (data) {
                        $('#status').fadeOut();
                        $('#preloader').delay(300).fadeOut('slow');
                        alert("E-mail enviado!");
                    },
                    error: function (jxhr, msg, err) {
                        $('#status').fadeOut();
                        $('#preloader').delay(300).fadeOut('slow');
                        alert("E-mail enviado!!");
                    }
                });
            } else {
                alert("Fill all the fields.");
            }

        }
        function validaCampos() {

            var validacao = true;

            if (document.getElementById("email").value == "") {
                validacao = false;
            }

            if (document.getElementById("message").value == "") {
                validacao = false;
            }

            if (document.getElementById("name").value == "") {
                validacao = false;
            }

            return validacao;
        }


    </script>

</head>

