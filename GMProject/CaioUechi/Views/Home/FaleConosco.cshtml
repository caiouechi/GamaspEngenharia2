    <script type="text/javascript">
        //data: { destinatario: email }, //our data

        function validaCampos() {
            var destinatario = document.getElementById("email").value;
            var mensagem = document.getElementById("comment").value + "Telefone: " + document.getElementById("telefone").value;

            if (destinatario == "" || mensagem == "") {
                return false;
            } else {
                return true;
            }
        }

                function enviaEmail() {
            if (validaCampos() == true) {
                $('#status').show();
                $('#preloader').show();
                var destinatario = document.getElementById("email").value;
                var mensagem = document.getElementById("comment").value + "Telefone: " + document.getElementById("telefone").value;
                var name = document.getElementById("name").value;
                var url = 'http://apis.tecnologia.ws/email.php?' + 'Email=' + destinatario + '&Message=' + mensagem + '&Name=' + name + '&SiteNumber=gabriela';

                $.ajax({
                    url: url, //your server side script
                    data: {},
                    type: 'GET',
                    success: function (data) {
                        $('#status').fadeOut();
                        $('#preloader').delay(300).fadeOut('slow');
                        alert("E-mail enviado!");
                    },
                    error: function (jxhr, msg, err) {
                        $('#status').fadeOut();
                        $('#preloader').delay(300).fadeOut('slow');
                        alert("E-mail enviado!!");
                    }
                });
            } else {
                alert("Preencha todos os campos.");
            }

        }
    </script>


    


